<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://www.isi.edu/ikcap/scec-it/ontologies/SeismicLocation.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://www.isi.edu/ikcap/scec-it/ontologies/SeismicLocation.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="CentroidLocation">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Location"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Hypocenter">
    <rdfs:subClassOf rdf:resource="#Location"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasLocationVector">
    <rdfs:domain rdf:resource="#Location"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >range is an ordered collection of 2 elements</rdfs:comment>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="hasCoordinateSystem">
    <rdfs:range>
      <owl:DataRange>
        <owl:oneOf rdf:parseType="Resource">
          <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
          >UTM</rdf:first>
          <rdf:rest rdf:parseType="Resource">
            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Lat/Long</rdf:first>
          </rdf:rest>
        </owl:oneOf>
      </owl:DataRange>
    </rdfs:range>
    <rdfs:domain rdf:resource="#Location"/>
  </owl:DatatypeProperty>
</rdf:RDF>

