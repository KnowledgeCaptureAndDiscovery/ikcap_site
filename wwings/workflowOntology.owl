<?xml version="1.0" encoding="UTF-8"?>

<rdf:RDF
    xmlns="http://www.isi.edu/ikcap/wwings/workflowOntology.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:flns="http://www.isi.edu/ikcap/wwings/fileOntology.owl#"
    xmlns:clns="http://www.isi.edu/ikcap/wwings/componentOntology.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="http://www.isi.edu/ikcap/wwings/workflowOntology.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.isi.edu/ikcap/wwings/componentOntology.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="WorkflowInstance">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Workflow"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="LinkList">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="Link"/>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#first"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
  </owl:Class>
  <owl:Class rdf:ID="LinkMapping">
    <rdfs:subClassOf rdf:resource="#Link"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >0</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="hasDestinationLinkMapping"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SystemLink">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasOriginFileDescription"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#DescriptionFile"/>
              <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#DescriptionFileCollection"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="OutputLink"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Node"/>
  <owl:Class rdf:ID="FileBinding"/>
  <owl:Class rdf:ID="MetadataForInputFile"/>
  <owl:Class rdf:ID="LogFile"/>
  <owl:Class rdf:ID="Ordering"/>
  <owl:Class rdf:ID="DAX"/>
  <owl:Class rdf:ID="WorkflowTemplate">
    <rdfs:subClassOf rdf:resource="#Workflow"/>
  </owl:Class>
  <owl:Class rdf:about="#OutputLink">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="hasDestinationNode"/>
            </owl:onProperty>
            <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >0</owl:maxCardinality>
          </owl:Restriction>
          <owl:Class rdf:about="#Link"/>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="hasDestinationFileDescription"/>
            </owl:onProperty>
            <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >0</owl:maxCardinality>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Execution"/>
  <owl:Class rdf:ID="InputLink">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >0</owl:maxCardinality>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:ID="hasOriginNode"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Class rdf:about="#Link"/>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasOriginFileDescription"/>
            </owl:onProperty>
            <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >0</owl:maxCardinality>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="ExecutableWorkflow"/>
  <owl:Class rdf:ID="InOutLink">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#hasDestinationNode"/>
            </owl:onProperty>
            <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:minCardinality>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty>
              <owl:FunctionalProperty rdf:about="#hasOriginNode"/>
            </owl:onProperty>
            <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
            >1</owl:minCardinality>
          </owl:Restriction>
          <owl:Class rdf:about="#Link"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasAfterNode">
    <rdfs:domain rdf:resource="#Ordering"/>
    <rdfs:range rdf:resource="#Node"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasComponent">
    <rdfs:domain rdf:resource="#Node"/>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/componentOntology.owl#ComponentType"/>
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/componentOntology.owl#ComponentCollection"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasLink">
    <rdfs:domain rdf:resource="#Workflow"/>
    <rdfs:range rdf:resource="#Link"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasInputFileMetadata">
    <rdfs:domain rdf:resource="#MetadataForInputFile"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.isi.edu/ikcap/wwings/fileOntology.owl#MetaDatatype"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasOriginFileDescription">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#File"/>
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#Collection"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:domain rdf:resource="#Link"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasExecutableWorkflow">
    <rdfs:range rdf:resource="#ExecutableWorkflow"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#WorkflowInstance"/>
          <owl:Class rdf:about="#Execution"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasFileBinding">
    <rdfs:range rdf:resource="#FileBinding"/>
    <rdfs:domain rdf:resource="#WorkflowInstance"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasInputLinks">
    <rdfs:range rdf:resource="#LinkList"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#LinkMapping"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasNode">
    <rdfs:range rdf:resource="#Node"/>
    <rdfs:domain rdf:resource="#Workflow"/>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:ID="hasLibraryFile">
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#File"/>
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#Collection"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:domain rdf:resource="#FileBinding"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#hasDestinationLinkMapping">
    <rdfs:range rdf:resource="#LinkMapping"/>
    <rdfs:domain rdf:resource="#Link"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasSkolemFile">
    <rdfs:domain rdf:resource="#FileBinding"/>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#File"/>
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#Collection"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasBeforeNode">
    <rdfs:range rdf:resource="#Node"/>
    <rdfs:domain rdf:resource="#Ordering"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#hasDestinationNode">
    <rdfs:domain rdf:resource="#Link"/>
    <rdfs:range rdf:resource="#Node"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="isSameMetadataAs">
    <rdfs:domain rdf:resource="#MetadataForInputFile"/>
    <rdfs:range rdf:resource="#MetadataForInputFile"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasDescription">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Workflow"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasFile">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Link"/>
          <owl:Class rdf:about="#MetadataForInputFile"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#File"/>
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#Collection"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasWorkflowTemplate">
    <rdfs:domain rdf:resource="#WorkflowInstance"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#WorkflowTemplate"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#hasDestinationFileDescription">
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#File"/>
          <rdf:Description rdf:about="http://www.isi.edu/ikcap/wwings/fileOntology.owl#Collection"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:domain rdf:resource="#Link"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#hasOriginNode">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Node"/>
    <rdfs:domain rdf:resource="#Link"/>
  </owl:FunctionalProperty>
  <owl:AnnotationProperty rdf:ID="hasSerialzation">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#Workflow"/>
  </owl:AnnotationProperty>
</rdf:RDF>

